.list-containter{
  border: 1px solid white;
  box-shadow: 0px 5px 5px rgb(138, 136, 136);
  margin: 5px 1px;
}
.list-label{
  font-size: 25rpx;
  line-height: 38rpx;
}
.dashed{
  border-bottom: 1px dashed rgb(255, 102, 0);
}

.list-label:nth-child(4) .label{
  display: inline-block;
  width: 33.33%;
}

.list-label:nth-child(5) .label{
  display: inline-block;
  width: 50%;
}

.list-label:last-child .label{
  float: right;
  background-color: rgb(37, 33, 33);
  border-radius: 4px;
  color: white;
  position: relative;
  padding: 1rpx 8rpx;
  right: 30rpx;
  bottom: 15rpx;
  
}